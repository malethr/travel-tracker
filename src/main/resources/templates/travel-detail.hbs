{{#partial "location"}}
   <p>Content:</p>
   <p>{{ travel.location }}</p>
   <h5>Created At:</h5>
   <p> {{ travel.createdAt }}</p>
   <h5>Post status:</h5>
   <p>
       {{#if travel.published }}
           <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
       {{else}}
           <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
       {{/if}}
   </p>
{{/partial}}

{{> layout.hbs}}